{
  "logging": {
    "level": "INFO",
    "file": "./logs/production_poi_expansion.log"
  },
  "backup": {
    "auto_backup": true,
    "compression": true,
    "max_backups": 15
  },
  "validation": {
    "strict_mode": true,
    "fail_fast": false
  },
  "performance": {
    "memory_limit_mb": 1024,
    "timeout_seconds": 600,
    "enable_profiling": false
  }
}