{
  "name": "100xfenok-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "sync-static": "cp -r ../100x ../alpha-scout ../tools ../ib ../posts ../admin ../data public/ 2>/dev/null || true; cp -r ../100x/data/* public/data/ 2>/dev/null || true; cp -rn ../alpha-scout/data/* public/data/ 2>/dev/null || true",
    "predev": "npm run sync-static",
    "dev": "next dev",
    "build": "npm run build:runtime",
    "build:runtime": "npm run sync-static && NEXT_BUILD_TARGET=runtime next build",
    "build:static": "npm run sync-static && NEXT_BUILD_TARGET=static next build",
    "start": "next start",
    "lint": "eslint",
    "qa:browser": "QA_BASE_URL=http://127.0.0.1:3105 node .qa-playwright.js",
    "qa:browser:lite": "QA_BASE_URL=http://127.0.0.1:3105 QA_VIEWPORTS=desktop QA_ROUTES=/,/tools/stock-analyzer/native node .qa-playwright.js",
    "qa:browser:dev": "QA_BASE_URL=http://127.0.0.1:3000 QA_DEV=1 node .qa-playwright.js",
    "qa:browser:dist": "QA_BASE_URL=http://127.0.0.1:4173 node .qa-playwright.js",
    "qa:lighthouse": "QA_BASE_URL=http://127.0.0.1:3105 QA_LIGHTHOUSE_RUNS=3 node .qa-lighthouse.js",
    "qa:lighthouse:lite": "QA_BASE_URL=http://127.0.0.1:3105 QA_LIGHTHOUSE_RUNS=1 QA_LIGHTHOUSE_PRESETS=desktop QA_LIGHTHOUSE_ROUTES=/tools/stock-analyzer/native node .qa-lighthouse.js",
    "qa:lighthouse:dev": "QA_BASE_URL=http://127.0.0.1:3000 QA_LIGHTHOUSE_RUNS=3 node .qa-lighthouse.js"
  },
  "dependencies": {
    "chart.js": "^4.5.1",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "19.2.3",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
