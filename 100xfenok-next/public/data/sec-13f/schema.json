{
  "folder": "sec-13f",
  "version": "3.0.1",
  "updated": "2026-02-20",
  "source": "SEC EDGAR 13F filings",
  "update_frequency": "quarterly (45 days after quarter end)",
  "maintainer": "SEC 13F Converter (accumulate mode)",

  "structure": {
    "summary.json": "Aggregated holdings summary",
    "by_sector.json": "Holdings by sector",
    "by_ticker.json": "Holdings by ticker",
    "investors/": "30 individual investor files",
    "analytics/": "Derived metrics (consensus, new positions, buying pressure, conviction)"
  },

  "files": {
    "summary.json": {
      "description": "Aggregated holdings summary across all investors",
      "size_kb": 6.2,
      "schema": {
        "last_updated": "YYYY-MM-DD",
        "total_aum": "number",
        "investor_count": "number",
        "top_holdings[]": {
          "ticker": "string",
          "name": "string",
          "total_value": "number",
          "investor_count": "number"
        }
      }
    },
    "by_sector.json": {
      "description": "Holdings aggregated by sector",
      "size_kb": 2.6,
      "schema": {
        "sectors.{sector_name}": {
          "total_value": "number",
          "weight": "number (0-1)",
          "top_stocks[]": {
            "ticker": "string",
            "value": "number"
          }
        }
      }
    },
    "by_ticker.json": {
      "description": "All holdings by ticker",
      "size_kb": 288,
      "schema": {
        "tickers.{TICKER}": {
          "name": "string",
          "total_value": "number",
          "holders[]": {
            "investor": "string",
            "shares": "number",
            "value": "number"
          }
        }
      }
    },
    "investors/{name}.json": {
      "description": "Individual investor holdings (30 files)",
      "investors": ["buffett", "marks", "klarman", "greenblatt", "einhorn", "pabrai", "gayner", "russo", "miller", "fidelity", "druckenmiller", "soros", "dalio", "tudor", "griffin", "cohen", "hohn", "laffont", "halvorsen", "coleman", "mandel", "ackman", "icahn", "peltz", "tepper", "loeb", "singer", "fisher", "asness", "wood"],
      "schema": {
        "metadata": {
          "version": "string",
          "quarters_covered": "string[] (e.g., 2025-Q3, 2025-Q2)"
        },
        "investor": {
          "name": "string",
          "entity": "string (fund name)",
          "cik": "string (SEC CIK number)",
          "group": "string (value/macro/hedge/tiger_cubs/activist/event_driven/quant/growth)"
        },
        "filings[]": {
          "quarter": "string (YYYY-QN)",
          "filing_date": "YYYY-MM-DD",
          "aum_total": "number",
          "holdings_count": "number",
          "top_10_weight": "number (0-1)",
          "holdings[]": {
            "ticker": "string",
            "name": "string",
            "shares": "number",
            "value": "number",
            "weight": "number (0-1)",
            "change": "string (NEW/INCREASED/DECREASED/SOLD)"
          }
        }
      }
    },
    "analytics/consensus.json": {
      "description": "Consensus score across all investors per ticker",
      "schema": {
        "tickers.{TICKER}": {
          "score": "number",
          "holder_count": "number",
          "total_value": "number"
        }
      }
    },
    "analytics/new_positions.json": {
      "description": "Newly initiated positions in latest quarter",
      "schema": {
        "positions[]": {
          "ticker": "string",
          "investor": "string",
          "value": "number",
          "quarter": "string"
        }
      }
    },
    "analytics/buying_pressure.json": {
      "description": "Net buying/selling pressure by ticker",
      "schema": {
        "tickers.{TICKER}": {
          "net_buyers": "number",
          "net_sellers": "number",
          "pressure_score": "number"
        }
      }
    },
    "analytics/conviction.json": {
      "description": "High-conviction positions (top weight per investor)",
      "schema": {
        "positions[]": {
          "ticker": "string",
          "investor": "string",
          "weight": "number",
          "value": "number"
        }
      }
    }
  },

  "investor_groups": {
    "value": ["buffett", "marks", "klarman", "greenblatt", "einhorn", "pabrai", "gayner", "russo", "miller", "fidelity"],
    "macro": ["druckenmiller", "soros", "dalio", "tudor"],
    "hedge": ["griffin", "cohen", "hohn", "laffont"],
    "tiger_cubs": ["halvorsen", "coleman", "mandel"],
    "activist": ["ackman", "icahn", "peltz"],
    "event_driven": ["tepper", "loeb", "singer"],
    "quant": ["fisher", "asness"],
    "growth": ["wood"]
  },

  "breaking_changes_history": [
    {"version": "3.0.1", "date": "2026-02-20", "change": "30 investors, 20Q accumulate mode, analytics/ folder added"},
    {"version": "2.1.0", "date": "2026-01-05", "change": "Added burry, pabrai investors"},
    {"version": "2.0.0", "date": "2025-12-01", "change": "Restructured with quarterly filings array"},
    {"version": "1.0.0", "date": "2025-11-01", "change": "Initial schema"}
  ]
}
