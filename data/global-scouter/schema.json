{
  "folder": "global-scouter",
  "version": "2.2.0",
  "updated": "2026-02-09",
  "source": "Custom aggregation (FinancialModelingPrep, Yahoo Finance)",
  "update_frequency": "on-demand",
  "maintainer": "Manual conversion scripts",

  "structure": {
    "core/": "Dashboard config, metadata, master stock index",
    "stocks/detail/": "1,240 individual stock profiles",
    "etfs/": "ETF and index benchmarks",
    "indicators/": "Economic indicators"
  },

  "files": {
    "core/dashboard.json": {
      "description": "Dashboard configuration",
      "size_kb": 3.1,
      "schema": {
        "config": "object (UI settings)"
      }
    },
    "core/metadata.json": {
      "description": "System metadata",
      "size_kb": 0.4,
      "schema": {
        "version": "string",
        "last_updated": "ISO timestamp"
      }
    },
    "core/stocks_index.json": {
      "description": "Master stock index (ticker -> basic info)",
      "size_kb": 333,
      "stock_count": 1240,
      "schema": {
        "stocks.{TICKER}": {
          "n": "string (name)",
          "x": "string (exchange)",
          "s": "string (sector)",
          "c": "string (country)",
          "p": "number (price)",
          "mc": "number (market_cap)",
          "pe": "number (PER forward)",
          "pb": "number (PBR forward)",
          "dy": "number (dividend yield)",
          "r12": "number (12M return)"
        }
      }
    },
    "stocks/detail/{TICKER}.json": {
      "description": "Individual stock profile (1,240 files)",
      "schema": {
        "years": "string[] (FY-4 to FY0)",
        "scale": {
          "market_cap": "number[]",
          "stock_price": "number[]",
          "total_assets": "number[]",
          "total_equity": "number[]"
        },
        "income_statement": {
          "revenue": "number[]",
          "gross_profit": "number[]",
          "operating_income": "number[]",
          "net_income": "number[]"
        },
        "cash_flow": {
          "cfo": "number[]",
          "capex": "number[]",
          "fcf": "number[]"
        },
        "profitability": {
          "gross_margin": "number[]",
          "operating_margin": "number[]",
          "roe": "number[]",
          "roa": "number[]"
        },
        "valuation": {
          "per": "number[]",
          "pbr": "number[]",
          "peg": "number[]"
        },
        "estimates": {
          "eps": {"fy1": "number", "fy2": "number", "fy3": "number"},
          "fcf": {"fy1": "number", "fy2": "number", "fy3": "number"},
          "revenue": {"fy1": "number", "fy2": "number", "fy3": "number"}
        },
        "fiscal_month": "string (Jan, Dec, etc.) - v2.2.0",
        "eps_consensus": {
          "fy_plus_1": "number",
          "fy_plus_2": "number",
          "fy_plus_3": "number",
          "_coverage": "100% (1,240 stocks) - v2.2.0"
        },
        "growth_consensus": {
          "revenue_7y": "number (%)",
          "revenue_3y": "number (%)",
          "earnings_7y": "number (%)",
          "earnings_3y": "number (%)",
          "_coverage": "100% (1,240 stocks) - v2.2.0"
        },
        "per_bands": {
          "current": "number",
          "min_8y": "number",
          "avg_8y": "number",
          "max_8y": "number",
          "source": "string (S_Valuation|calculated)",
          "_coverage": "95.8% (1,188 stocks) - v2.2.0"
        }
      }
    },
    "etfs/index.json": {
      "added_in": "2.1.0",
      "description": "23 ETFs/Indices with returns, CAGR, market cap",
      "size_kb": 13,
      "etf_count": 23,
      "schema": {
        "count": "number",
        "etfs.{name}": {
          "ticker": "string",
          "category": "string (Indices/ETF)",
          "market_cap": "number",
          "returns": {
            "1m": "number",
            "3m": "number",
            "ytd": "number",
            "1y": "number"
          },
          "cagr": {
            "3y": "number",
            "5y": "number",
            "10y": "number"
          }
        }
      }
    },
    "indicators/economic.json": {
      "added_in": "2.1.0",
      "description": "Treasury rates, spreads, HY yields, TIPS",
      "record_count": 1048,
      "schema": {
        "records[]": {
          "date": "YYYY-MM-DD",
          "t10y": "number",
          "t2y": "number",
          "t10y_2y_spread": "number",
          "hys_us": "number",
          "bei_10y": "number",
          "tips_10y": "number"
        }
      }
    }
  },

  "breaking_changes_history": [
    {"version": "2.2.0", "date": "2026-01-25", "change": "Extended fields: fiscal_month, eps_consensus, growth_consensus, per_bands"},
    {"version": "2.1.0", "date": "2026-01-10", "change": "etfs/ and indicators/ folders added"},
    {"version": "2.0.0", "date": "2025-12-15", "change": "stocks/detail/ restructured"},
    {"version": "1.0.0", "date": "2025-11-01", "change": "Initial schema"}
  ]
}
